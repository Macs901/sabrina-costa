{
  "name": "sabrina-costa-docs",
  "version": "1.0.0",
  "description": "Documentação do Dashboard Sabrina Costa com auto-geração de API docs",
  "main": "index.js",
  "scripts": {
    "docs:generate": "node scripts/generate-api-docs.js",
    "docs:serve": "python -m http.server 8080",
    "docs:build": "npm run docs:generate",
    "docs:watch": "nodemon scripts/generate-api-docs.js",
    "docs:validate": "swagger-codegen validate -i api/openapi.yaml",
    "docs:deploy": "npm run docs:build && echo 'Documentação pronta para deploy'",
    "install:docs": "npm install swagger-jsdoc swagger-codegen-cli nodemon --save-dev",
    "setup:docs": "npm run install:docs && npm run docs:generate",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "changelog:version": "npm run changelog && git add CHANGELOG.md",
    "changelog:generate": "node scripts/generate-changelog.js",
    "changelog:setup": "node scripts/setup-changelog.js",
    "commit": "git-cz",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "version:setup": "node scripts/setup-versioning.js",
    "version:create": "node scripts/create-version.js",
    "version:switch": "node scripts/switch-version.js",
    "version:list": "node scripts/list-versions.js",
    "version:validate": "node scripts/validate-version.js",
    "version:migrate": "node scripts/migrate-version.js"
  },
  "keywords": [
    "documentation",
    "api",
    "swagger",
    "openapi",
    "sabrina-costa",
    "dashboard"
  ],
  "author": "Marco Cardoso <marco@example.com>",
  "license": "MIT",
  "devDependencies": {
    "swagger-jsdoc": "^6.2.8",
    "swagger-codegen-cli": "^3.0.0",
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Marcocardoso91/sabrina-costa.git",
    "directory": "docs"
  },
  "bugs": {
    "url": "https://github.com/Marcocardoso91/sabrina-costa/issues"
  },
  "homepage": "https://github.com/Marcocardoso91/sabrina-costa#readme"
}
